{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Console": {
      "IncludeScopes": true
    },
    "File": {
      "IncludeScopes": true,
      "BasePath": "Logs",
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "Files": [
        {
          "Path": "app.log",
          "MaxFileSize": 10000
        }
      ]
    }
  }
}
